{
    "cloudSql": {
        "ConnectionString": "Host=geofences_postgres;Port=5432;Username=postgres;Password=postgres;Database=geofences"
    },
    "DataProtectionCertPath": {
        "Path": "./Certs/geofences-protection-cert.pfx"
    },
    "rabbitMQ": {
        "host": "rabbitmq",
        "port": "5672",
        "virtualHost": "/",
        "username": "geofencesUser",
        "password": "geofencesPass",
        "namespace": "geofences",
        "retries": 3,
        "retryInterval": 3
    },
    "mongo": {
        "connectionString": "mongodb://geofencesUser:geofencesPass@geofences_mongo:27017/geofences?authSource=geofences",
        "defaultDatabase": "geofences"
    },
    "Serilog": {
        "Using": ["Serilog.Sinks.Console"],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Warning"
            }
        },
        "Enrich": ["FromLogContext"],
        "WriteTo": [
            {
                "Name": "DatadogLogs",
                "Args": {
                    "apiKey": "",
                    "service": "Geofences",
                    "source": "Serilog",
                    "host": "Nick's Laptop"
                }
            }
        ]
    }
}
